    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.8",
    "@types/nodemailer": "^6.4.15",
    "@types/validator": "^13.11.9",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
